      "source": [
        "# create new column with number of tokens of cleaned texts\n",
        "def list_length(list):\n",
        "  length=len(list)\n",
        "  return length\n",
        "\n",
        "dischargeNote['num_of_cleaned_tokens']=remove_num_clean_words.apply(list_length)"
