      "source": [
        "# remove numbers function\n",
        "def remove_numbers(words):\n",
        "  numbers='0123456789'\n",
        "  clean_words=[]\n",
        "  for word in words:\n",
        "    if word not in numbers:\n",
        "      clean_words.append(word.lower())\n",
        "  return clean_words\n",
        "\n",
        "\n",
        "\n"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": 69,
